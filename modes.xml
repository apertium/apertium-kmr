<?xml version="1.0" encoding="UTF-8"?>
<modes>

  <mode name="kur-morph">
    <pipeline>
      <program name="lt-proc -w">
        <file name="kur.automorf.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="kur-gener">
    <pipeline>
      <program name="lt-proc -g">
        <file name="kur.autogen.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="kur-disam">
    <pipeline>
      <program name="lt-proc -w">
        <file name="kur.automorf.bin"/>
      </program>
      <program name="cg-proc -w">
        <file name="kur.rlx.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="kur-disam-debug">
    <pipeline>
      <program name="lt-proc -w">
        <file name="kur.automorf.bin"/>
      </program>
      <program name="cg-conv -a -l" />
      <program name="vislcg3 --trace -g">
        <file name="apertium-kur.kur.rlx"/>
      </program>
    </pipeline>
  </mode>


</modes>
